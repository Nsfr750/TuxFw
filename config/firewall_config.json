{
  "firewall_rules": [
    {
      "id": "rule_1",
      "name": "Allow HTTP",
      "description": "Allow incoming HTTP traffic",
      "protocol": "TCP",
      "port": "80",
      "direction": "IN",
      "action": "ALLOW",
      "enabled": true,
      "created_date": "2025-01-27"
    },
    {
      "id": "rule_2",
      "name": "Allow HTTPS",
      "description": "Allow incoming HTTPS traffic",
      "protocol": "TCP",
      "port": "443",
      "direction": "IN",
      "action": "ALLOW",
      "enabled": true,
      "created_date": "2025-01-27"
    },
    {
      "id": "rule_3",
      "name": "Block SSH",
      "description": "Block incoming SSH connections",
      "protocol": "TCP",
      "port": "22",
      "direction": "IN",
      "action": "BLOCK",
      "enabled": true,
      "created_date": "2025-01-27"
    },
    {
      "id": "rule_4",
      "name": "Allow DNS",
      "description": "Allow DNS queries",
      "protocol": "UDP",
      "port": "53",
      "direction": "OUT",
      "action": "ALLOW",
      "enabled": true,
      "created_date": "2025-01-27"
    }
